    "def get_hashtags(text, handle_space=True):\n",
    "\n",
    "\n",
    "    if handle_space:\n",
    "        text = re.sub('[^a-zA-Z#]', '', text)\n",
    "        space = r\"#\\s+\"\n",
    "        text = re.sub(space, \"#\", str(text).lower()).strip()\n",
    "    \n",
    "    hash_tags = r\"#\\S+\"\n",
    "    \n",
    "    return \" \".join(re.findall(hash_tags, text))\n",
    "\n",
    "df_train['hashtags_negative'] = df_train[df_train['sentiment']==0]['tweet'].apply(get_hashtags)\n",
    "\n",
    "# cdf\n",
    "\n",
    "df_train['hashtags_negative'].head()\n",
